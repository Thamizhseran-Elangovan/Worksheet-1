#include <iostream>
#include <string>
using namespace std;

int main() {
    string pass = "psg123", input;
    do {
        cout << "Enter password: ";
        cin >> input;
        if (input == pass) {
            cout << "Logged In" << endl;
            break;
        } else {
            cout << "Incorrect Password. Try again\n";
        }
    } while (true);

    return 0;
}
